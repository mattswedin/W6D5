<h1> All Cats! Yay! </h1>

<% @cats.each do |cat|%>
    <ul> 
        <li>name: <%= cat.name %></li>
        <li>birth date: <%= cat.birth_date %></li>
        <li>color: <%= cat.color %></li>
        <li>sex: <%= cat.sex %></li>
        <li>description: <%= cat.description %></li>
        <li> 
            <a href="<%=cat_url(cat)%>" >Show Cat</a>
        </li>
    </ul>
<% end %>

